        <RelativeLayout
            android:id="@+id/relativeLayout"
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:layout_alignParentBottom="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:paddingBottom="10dp"
            android:paddingTop="10dp">

            <ImageButton
                android:id="@+id/Btn_power"
                android:layout_width="80dp"
                android:layout_height="80dp"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:background="@drawable/back_rojo"
                android:src="@drawable/location_off" />

            <ImageButton
                android:id="@+id/Btn_openconfig"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_centerVertical="true"
                android:layout_marginLeft="20dp"
                android:layout_toEndOf="@+id/Btn_power"
                android:layout_toRightOf="@+id/Btn_power"
                android:background="@drawable/back_subbuttons"
                android:src="@drawable/settings" />

            <ImageButton
                android:id="@+id/Btn_openLog"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_centerVertical="true"
                android:layout_marginRight="20dp"
                android:layout_toLeftOf="@+id/Btn_power"
                android:layout_toStartOf="@+id/Btn_power"
                android:background="@drawable/back_subbuttons"
                android:src="@drawable/download3" />

        </RelativeLayout>


